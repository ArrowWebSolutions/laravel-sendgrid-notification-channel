{
    "name": "swiftmade/laravel-notification-sendgrid-channel",
    "description": "Laravel Notification Channel for Sengrid.com",
    "homepage": "https://github.com/laravel-notification-channels/sendgrid",
    "license": "MIT",
    "authors": [
        {
            "name": "Ahmet Ã–zisik",
            "email": "hello@swiftmade.co",
            "homepage": "https://swiftmade.co",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=7.2",
        "illuminate/notifications": "~5.5 || ~6.0 || ~7.0",
        "illuminate/support": "~5.5 || ~6.0 || ~7.0",
        "sendgrid/sendgrid": "^7.8"
    },
    "require-dev": {
        "mockery/mockery": "^1.3",
        "phpunit/phpunit": "^8.0"
    },
    "autoload": {
        "psr-4": {
            "NotificationChannels\\SendGrid\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "NotificationChannels\\SendGrid\\Test\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit",
        "test:coverage": "phpunit --coverage-text --coverage-clover=coverage.clover"
    },
    "config": {
        "sort-packages": true
    }
}
